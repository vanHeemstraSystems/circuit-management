//registry.npmjs.org/:_auth=<username>:<password> base64 encoded
//registry.npmjs.org/:_authToken=<npm_access_token>
//registry.npmjs.org/:username=<username>
//registry.npmjs.org/:_password=<password>
email=<email>
always-auth=true
strict-ssl=false
registry=https://registry.npmjs.org
registry.npmjs.org/:_authToken=<npm_access_token>
proxy=http://<username>:<password>@<proxy-hostname>:<proxy-port>
http-proxy=http://<username>:<password><proxy-hostname>:<proxy-port>
https-proxy=http://<username>:<password>@<proxy-hostname>:<proxy-port>